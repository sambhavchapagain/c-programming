#include<stdio.h>

int main()
{
    int a[3][3], b[3][3],i,j,sum[3][3];
    printf("addition for 3 by 3 matrix. \n");
    for(i=0; i<3; i++)
    {
        for(j=0; j<3; j++)
        {
            printf("enter the value of first matrix :");
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<3;i++)
    {
        for(j=0; j<3; j++)
        {
            printf("enter the value of second matrix: ");
            scanf("%d",&b[i][j]);
        }
    }
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            sum[i][j]=a[i][j]+b[i][j];
        }
    }
printf("the sum is :\n");
for(i=0;i<3;i++)
{
    for(j=0;j<3;j++)
    {
        printf(" %d\t",sum[i][j]);
    }
    printf(" \n");
  }
return 0;
}